Drop Table IF Exists food; 

Create Table food (
	name varchar,
	image varchar,
	class varchar,
	subclass varchar,
	description varchar,
	ingredients varchar, 
	hp float, 
	effect varchar

); 

Select * from food; 

Select 
	name, 
	class, 
	subclass, 
	description, 
	ingredients, 
	hp, 
	effect
From food ; 
